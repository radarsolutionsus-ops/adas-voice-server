  "calibration_triggers": {
    "critical": [
      {
        "repair_keywords": ["windshield", "windscreen", "front glass", "laminated glass", "w/s"],
        "sensors_affected": ["forward_camera", "rain_sensor", "humidity_sensor"],
        "calibration_type": "static_and_dynamic",
        "priority": "CRITICAL",
        "note": "89% of 2023+ vehicles require calibration after windshield replacement"
      },
      {
        "repair_keywords": ["front bumper", "fr bumper", "front fascia", "front cover", "bumper cover front"],
        "sensors_affected": ["front_radar", "acc_sensor", "front_parking_sensors", "aeb_sensor"],
        "calibration_type": "static_and_dynamic",
        "priority": "CRITICAL",
        "note": "Even R&I of same bumper can misalign hidden sensors"
      },
      {
        "repair_keywords": ["grille", "grill", "front grille", "radiator grille"],
        "sensors_affected": ["front_radar", "acc_sensor"],
        "calibration_type": "static_and_dynamic",
        "priority": "CRITICAL",
        "note": "Radar typically mounted behind brand emblem in grille"
      },
      {
        "repair_keywords": ["rear bumper", "rr bumper", "rear fascia", "rear cover", "bumper cover rear"],
        "sensors_affected": ["rear_radar", "bsm_sensors", "rcta_sensors", "rear_parking_sensors"],
        "calibration_type": "static_and_dynamic",
        "priority": "CRITICAL",
        "note": "BSM and RCTA sensors located in rear bumper corners"
      },
      {
        "repair_keywords": ["collision", "accident", "impact", "crash"],
        "sensors_affected": ["all_adas_sensors"],
        "calibration_type": "pre_post_scan_plus_calibration",
        "priority": "CRITICAL",
        "note": "All collision events require pre and post repair scanning regardless of severity"
      },
      {
        "repair_keywords": ["frame", "structural", "unibody", "rail", "apron", "strut tower"],
        "sensors_affected": ["all_adas_sensors"],
        "calibration_type": "static_and_dynamic",
        "priority": "CRITICAL",
        "note": "Frame repairs alter vehicle geometry affecting all sensor angles"
      },
      {
        "repair_keywords": ["airbag", "air bag", "srs", "restraint", "deployed"],
        "sensors_affected": ["steering_angle_sensor", "occupant_sensors", "forward_camera"],
        "calibration_type": "static",
        "priority": "CRITICAL",
        "note": "Airbag deployment requires steering angle calibration"
      },
      {
        "repair_keywords": ["camera replace", "radar replace", "sensor replace", "module replace"],
        "sensors_affected": ["per_component"],
        "calibration_type": "per_oem",
        "priority": "CRITICAL",
        "note": "Any sensor/camera replacement always requires calibration"
      }
    ],
    "high": [
      {
        "repair_keywords": ["suspension", "strut", "shock", "control arm", "ball joint", "tie rod"],
        "sensors_affected": ["steering_angle_sensor", "forward_camera", "yaw_sensor"],
        "calibration_type": "static",
        "priority": "HIGH",
        "note": "Changes in ride height affect camera field of view"
      },
      {
        "repair_keywords": ["alignment", "wheel alignment", "4 wheel alignment", "thrust angle"],
        "sensors_affected": ["steering_angle_sensor"],
        "calibration_type": "static",
        "priority": "HIGH",
        "note": "Steering angle zero-point calibration required"
      },
      {
        "repair_keywords": ["quarter panel", "qtr panel", "rear quarter", "rear fender"],
        "sensors_affected": ["bsm_sensors"],
        "calibration_type": "static",
        "priority": "HIGH",
        "note": "BSM sensors often located in rear quarter panel area"
      },
      {
        "repair_keywords": ["mirror", "side mirror", "door mirror", "wing mirror", "o/s mirror"],
        "sensors_affected": ["bsm_camera", "lane_change_camera", "360_camera"],
        "calibration_type": "static",
        "priority": "HIGH",
        "note": "Many BSM cameras and 360 view cameras in mirrors"
      },
      {
        "repair_keywords": ["headlamp", "headlight", "head lamp", "adaptive headlight"],
        "sensors_affected": ["adaptive_headlights", "auto_high_beam"],
        "calibration_type": "static",
        "priority": "HIGH",
        "note": "Adaptive and auto-leveling headlights require aiming"
      },
      {
        "repair_keywords": ["tailgate", "liftgate", "hatch", "trunk lid"],
        "sensors_affected": ["rear_camera", "360_camera"],
        "calibration_type": "static",
        "priority": "HIGH",
        "note": "Rear camera and 360 view camera often mounted in tailgate"
      }
    ],
    "conditional": [
      {
        "repair_keywords": ["hood", "bonnet"],
        "check_for": "forward_camera_obstruction",
        "calibration_if": "camera_affected",
        "priority": "CONDITIONAL"
      },
      {
        "repair_keywords": ["fender", "front fender", "wing"],
        "check_for": "side_sensors_in_fender",
        "calibration_if": "sensors_present",
        "priority": "CONDITIONAL"
      },
      {
        "repair_keywords": ["door", "front door", "rear door"],
        "check_for": "side_impact_sensors_bsm_cameras",
        "calibration_if": "per_oem",
        "priority": "CONDITIONAL"
      },
      {
        "repair_keywords": ["roof", "roof panel"],
        "check_for": "roof_mounted_sensors",
        "calibration_if": "sensors_present",
        "priority": "CONDITIONAL",
        "note": "Some EVs have roof-mounted LiDAR or GPS antennas"
      },
      {
        "repair_keywords": ["tire", "wheel", "tire size"],
        "check_for": "speed_dependent_adas",
        "calibration_if": "oem_requires",
        "priority": "CONDITIONAL"
      }
    ]
  },

